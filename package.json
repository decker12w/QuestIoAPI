    "prisma": "^5.14.0",
    "reflect-metadata": "^0.2.2",
    "supertest": "^7.0.0",
    "tsx": "^4.10.5",
    "tsyringe": "^4.8.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.11.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
